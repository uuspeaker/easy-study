<view class="page">
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">

            <view url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" >
            <view class="weui-cell__hd">
                <image src="{{item.imageUrl}}"  mode='aspectFit'></image>
                <view class="content-block">
                  <image src="../../../images/triangle-right.png"  mode='aspectFit' class="tinyImg"></image>
                  <div class="tiny-title">题目</div>
                  <view class="line"></view>
                </view>
                
                <view wx:for="{{item.content}}" wx:for-item="lineStr" class="main-content">{{lineStr}}</view>
                <view class="content-block">
                  <image src="../../../images/triangle-right.png"  mode='aspectFit' class="tinyImg"></image>
                  <div class="tiny-title">解析</div>
                  <view class="line"></view>
                </view>

                <view class="content-block">
                  <image src="../../../images/triangle-right.png"  mode='aspectFit' class="tinyImg"></image>
                  <div class="tiny-title">答案</div>
                  <view class="line"></view>
                </view>
               
                <view class="content-block">
                  <image src="../../../images/triangle-right.png"  mode='aspectFit' class="tinyImg"></image>
                  <div class="tiny-title">点评</div>
                  <view class="line"></view>
                </view>
            </view>
            
            
            </view> 
            

        </view>
    </view>
</view>



<template name="dataTemplate">
    <view url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active" >
        <view class="weui-cell__hd">
            <image src="{{imageUrl}}"  mode='aspectFit'></image>
            <view>{{}}</view>
        </view>
        <view class='weui-flex__item' >
        <block wx:for="{{subjectContent}}" >
          {{item}}
        </block>
         
        </view>
    </view> 
</template>
 

